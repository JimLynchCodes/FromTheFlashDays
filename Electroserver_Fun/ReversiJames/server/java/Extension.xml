<?xml version="1.0" encoding="utf-8" ?>
<Extension>
    <Name>Reversi</Name>
    <EventHandlers>
       <ExtensionLifecycleHandlers>
            <ExtensionLifecycleHandler>
                <Handle>GMSInitializer</Handle>
                <Type>Java</Type>
                <ServerLevel>1</ServerLevel>
                <Path>com.electrotank.electroserver5.examples.gamemanager.GMSInitializer</Path>
            </ExtensionLifecycleHandler>
        </ExtensionLifecycleHandlers>
    </EventHandlers>
    <Plugins>
        <Plugin>
            <Handle>ReversiLobby</Handle>
            <Type>Java</Type>
            <Path>com.electrotank.electroserver5.examples.reversi.Lobby</Path>
        </Plugin>
        <Plugin>
            <Handle>Reversi</Handle>
            <Type>Java</Type>
            <Path>com.electrotank.electroserver5.examples.reversi.Reversi</Path>
        </Plugin>
    </Plugins>
</Extension>

